nav
	.nav
		- var pages = { Home: '/', About: '/about' };
		each menu, key in pages
			if (key == pageName)
				.active.left.nav-item
					a(href=`${menu}`)= key
			else
				.left.nav-item
					a(href=`${menu}`)= key
		if (pageName == "Dogs" || pageName == "Top 3")
			#dropdown.left.nav-item.active
				a Doggos ⏬
				#dropdown_menu(style="display: none")
					a(href="/dogs") Top 10
					a(href="/top") Top 3
		else
			#dropdown.left.nav-item
				a Doggos ⏬
				#dropdown_menu(style="display: none")
					a(href="/dogs") Top 10
					a(href="/top") Top 3
		.right.nav-item
			a.switchA
				label.switch
					input#colorPreferance(type="checkbox")
					span.slider.round(id=slider)
